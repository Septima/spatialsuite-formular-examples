<config>
   <formular name="ventilationsanlaeg">
        <header>Ventilationsanlæg</header>
        <subheader> </subheader>
		<submitpages>
			<page condition="jQuery('#rediger').val() == 'false'">formular.send.ventilationsanlaeg</page>			
